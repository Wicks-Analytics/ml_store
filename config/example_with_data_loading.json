{
  "model_type": "classifier",
  "target_column": "target",
  "data_path": "data/train_data.csv",
  "data_type": "csv",
  "data_options": {
    "separator": ",",
    "null_values": ["NA", "NULL", ""],
    "infer_schema_length": 10000
