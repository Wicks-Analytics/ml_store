  "model_params": {
    "learning_params": {
      "iterations": 1000,
      "learning_rate": 0.1,
      "depth": 6,
      "loss_function": "Logloss",
      "eval_metric": "AUC",
      "verbose": 100
    },
    "early_stopping_rounds": 50
  },
  "mlflow": {
    "experiment_name": "classification_experiment",
    "run_name": "catboost_classifier_v1"
